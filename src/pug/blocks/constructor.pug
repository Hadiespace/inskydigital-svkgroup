section.constructor
	div.constructor__container.container
		h2.visually-hidden Конструктор
		div.constructor__sticky
			ul.constructor__list
				each image in constructorImages
					li.constructor__item
						img(src=image.src, alt=image.alt)
			div.constructor__swiper
				div.constructor__wrapper.swiper
					ul.constructor__slides.swiper-wrapper
						each slide in constructorSlides
							li.constructor__slide.swiper-slide
								h3!= slide.title
								div
									p!= slide.content
				div.constructor__info

